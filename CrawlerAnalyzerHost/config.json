{
  "loglevel": "info",
  "logdir": "./logs",
  "services": [
    {
      "name": "WebCrawler",
      "loglevel": "info",
      "periodic": true,
      "interval": 900,
      "command": "node",
      "args": [
        "index.js"
      ],
      "options": {
        "path": "../Crawlers/js/WebCrawler",
        "envvars": null,
        "shell": null
      }
    },
    {
      "name": "TwitterCrawker",
      "loglevel": "info",
      "periodic": true,
      "interval": 900,
      "command": "python3",
      "args": [
        "Main.py"
      ],
      "options": {
        "path": "../Crawlers/python",
        "envvars": null,
        "shell": null
      }
    }
  ]
}